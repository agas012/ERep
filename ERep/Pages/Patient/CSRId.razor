    protected override async Task OnParametersSetAsync()
    {
        if (Idc == null)
        {
            patient.CSRs = PatientService.CSRs;
        }
        else
        {
            patient = await PatientService.GetSinglePatient((int)Id);
            crs = await PatientService.GetSingleCSR((int)Id, (int)Idc);
        }
    }


    async void Onsubmit(ERep.Models.CSR csr)
    {
        if (Idc == null)
        {
            crs.PatientId = (int)Id;
            await PatientService.CreateCSR(crs, (int)Id);
        }
        else
        {
            await PatientService.UpdateCSR(crs, (int)Id, (int)Idc);
            //await PatientService.UpdateCaseQuestions(crs.caseQuestions,(int)Id);
        }

    }

    void Cancel()
    {
        NavigationManager.NavigateTo($"/csr/{Id}");
    }

    async void Delete()
    {
        if(Idc != null)
            await PatientService.DeleteCSR(crs, (int)Id, (int)Idc);
    }


    private void ClearSelection(Action<int> setValue)
    {
        setValue(0);
    }
