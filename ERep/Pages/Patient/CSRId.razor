@page "/csrId/{Id:int}"
@page "/csrId/{Id:int}/{Idc:int}"
@using Microsoft.AspNetCore.Http.Features;
@inject ERep.Data.IPatientService PatientService
@inject NavigationManager NavigationManager

<style>
    .fixed-button-borrar {
        position: fixed;
        top: 10px; /* Adjust as needed */
        right: 10px; /* Adjust as needed */
        z-index: 1000; /* Ensures the button is above other elements */
    }
    .fixed-button-cancelar {
        position: fixed;
        top: 10px; /* Adjust as needed */
        right: 150px; /* Adjust as needed */
        z-index: 1000; /* Ensures the button is above other elements */
    }
    .fixed-button-guardar {
        position: fixed;
        top: 10px; /* Adjust as needed */
        right: 300px; /* Adjust as needed */
        z-index: 1000; /* Ensures the button is above other elements */
    }
</style>

<RadzenTemplateForm Data=@crs TItem="ERep.Models.CSR" Submit="@Onsubmit">
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-md-12 d-flex align-items-end justify-content-center" style="margin-top: 16px;">
                <RadzenButton Class="fixed-button-guardar" ButtonType="ButtonType.Submit" Icon="save" Text="Guardar" />
                <RadzenButton Class="fixed-button-cancelar " ButtonStyle="ButtonStyle.Light" Icon="cancel" style="display: inline-block; margin-left: 10px;" Text="Cancelar" Click="@Cancel" />
                <RadzenButton Class="fixed-button-borrar" ButtonStyle="ButtonStyle.Warning" Icon="delete" style="display: inline-block; margin-left: 10px;" Text="Borrar" Click="@Delete" />
            </div>
        </div>
    </div>

    <div class="container-fluid">
        <div class="row justify-content-center">   
            <div class="col-lg-6 col-xl-4 p-3">
                <RadzenCard>
                    <RadzenLabel Text="Fecha de encuesta" Component="Date" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenDatePicker @bind-Value=@crs.Date Name="Date" ShowTime="false" ShowSeconds="false" HoursStep="1.0" MinutesStep="5" SecondsStep="10" DateFormat="dd/MM/yyyy" Class="w-100" />
                </RadzenCard>
            </div>
        </div>
        <RadzenLabel Text="SECCIÓN 2: DATOS DE LA ENFERMEDAD" Size="H2" class="rz-card-title" />
        <RadzenCard>
            <div class="row justify-content-center">
                <div class="col-lg-6 col-xl-4 p-3">
                    <RadzenCard>
                        <RadzenLabel Text="Enfermedad reumática previamente diagnosticada" Component="ereumatica" Style="margin-left: 8px; vertical-align: middle;" />
                        <RadzenDropDown TValue="int" TextProperty="Key" ValueProperty="Value"
                                @bind-Value="@crs.EreumaticaId"
                                Data="@crs.Ereumatica"
                                Style="width: 100%">
                        </RadzenDropDown>
                    </RadzenCard>
                    @if (crs.EreumaticaId==12)
                    {
                         <div class="col-lg-6 col-xl-4 p-3">
                            <RadzenCard>
                                <RadzenTextBox Placeholder="escribir cual" @bind-Value="@crs.otraEreumatica"/>
                            </RadzenCard>
                        </div> 
                    }
                </div>
                <div class="col-lg-6 col-xl-4 p-3">
                    <RadzenCard>
                        <RadzenLabel Text="¿Escriba cuándo iniciaron sus síntomas?" Component="DateSintomas" Style="margin-left: 8px; vertical-align: middle;" />
                        <RadzenDatePicker @bind-Value=@crs.DateSintomas Name="DateSintomas" ShowTime="false" ShowSeconds="false" HoursStep="1.0" MinutesStep="5" SecondsStep="10" DateFormat="dd/MM/yyyy" Class="w-100" />
                    </RadzenCard>
                </div>  
                <div class="col-lg-6 col-xl-4 p-3">
                    <RadzenCard>
                        <RadzenLabel Text="¿Enqué fecha su reumatólogo le dio un diagnóstico?" Component="DateDiagnostico" Style="margin-left: 8px; vertical-align: middle;" />
                        <RadzenDatePicker @bind-Value=@crs.DateDiagnostico Name="DateDiagnostico" ShowTime="false" ShowSeconds="false" HoursStep="1.0" MinutesStep="5" SecondsStep="10" DateFormat="dd/MM/yyyy" Class="w-100" />
                    </RadzenCard>
                </div> 
            </div>
            <div class="row justify-content-center">
	                <RadzenCard>
		                <RadzenLabel Text="¿Actualmente ¿Toma alguno de estos medicamentos?" Size="H4" class="rz-card-title" /> 
                        <p>
		                    <RadzenLabel Text="(Se puede circular más de una respuesta)" />
		                    <RadzenLabel Text="(Entre paréntesis puede ver los nombres comerciales más comunes)" />
                        </p>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_1" Text="Metotrexato" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_1  Name="medicamento_1" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_2" Text="Mofetil micofenolato/Acido micofenólico	" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_2  Name="medicamento_2" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_3" Text="Ciclofosfamida (Genoxal)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_3  Name="medicamento_3" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_4" Text="Talidomida" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_4  Name="medicamento_4" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_5" Text="Leflunomida (Arava, Almura, Filarin o Galdione)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_5  Name="medicamento_5" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_6" Text="Sulfasalazina (Azulfidina)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_6  Name="medicamento_6" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_7" Text="Rituximab (Mabthera)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_7  Name="medicamento_7" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_8" Text="Belimumab (Benlystia)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_8  Name="medicamento_8" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_9" Text="Azatioprina (Inmuran o Azatrilem)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_9  Name="medicamento_9" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_10" Text="Cloroquina (Aralen)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_10  Name="medicamento_10" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_11" Text="Hidroxicloroquina (Plaquenil, Himanix o Himanex)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_11  Name="medicamento_11" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_12" Text="Prednisona (Meticorten o Deflazacort" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_12  Name="medicamento_12" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_13" Text="Tacrolimus (Prograf)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_13  Name="medicamento_13" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_14" Text="Ácido fólico (Folivital) " Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_14  Name="medicamento_14" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_15" Text="Vitamina D	" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_15  Name="medicamento_15" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_16" Text="Ninguno" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_16  Name="medicamento_16" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_17" Text="Antinflamatorios no esteroideos (AINES)" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_17  Name="medicamento_17" TValue="bool"/>
		                </RadzenCard>
		                <RadzenCard>
			                <RadzenLabel Component="medicamento_otros" Text="Otros" Style="margin-left: 8px; vertical-align: middle;" />
			                <RadzenCheckBox @bind-Value=@crs.medicamento_otros  Name="medicamento_otros" TValue="bool"/>
		                </RadzenCard>		
		                @if (crs.medicamento_otros)
		                {
			                <RadzenCard>	
				                <RadzenTextBox @bind-Value="@crs.medicamento_otros_text" Style="width: 100%; margin-top: 10px;" Placeholder="Otros" />
			                </RadzenCard>
		                }
	                </RadzenCard>
            </div>
        </RadzenCard>

        @*Relación con su reumatólogo*@
        <RadzenLabel Text="SECCIÓN 3: RELACIÓN CON SU REUMATÓLOGO" Size="H2" class="rz-card-title" />
        <RadzenCard>
            <RadzenCard>
                <RadzenLabel Text="¿Que tanta confianza tiene para hablar con su reumatólogo para comentarle sobre temas como planificación familiar (deseo de embarazo) y consejería sobre métodos anticonceptivos?" Size="H3" class="rz-card-title" />
                <RadzenDropDown TValue="int" TextProperty="Key" ValueProperty="Value"
                @bind-Value="@crs.reuma_confianzaplanificarfamiliaId"
                                Data="@crs.NadaMucho"
                                Style="width: 100%">
                </RadzenDropDown>
            </RadzenCard>
            <RadzenCard>
                <RadzenLabel Text="¿Con que frecuencia ha recibido por parte de su reumatólogo consejería sobre planificación familiar (deseo de embarazo) y consejería sobre métodos anticonceptivos ?" Size="H3" class="rz-card-title" />
                <RadzenDropDown TValue="int" TextProperty="Key" ValueProperty="Value"
                @bind-Value="@crs.reuma_planificarfamiliaId"
                                Data="@crs.NuncaSiempre"
                                Style="width: 100%">
                </RadzenDropDown>
            </RadzenCard>
            <RadzenCard>
                <RadzenLabel Text="¿Con que frecuencia su reumatólogo le ha dicho de los riesgos de embarazarse en este momento respecto a su enfermedad?" Size="H3" class="rz-card-title" />
                <RadzenDropDown TValue="int" TextProperty="Key" ValueProperty="Value"
                @bind-Value="@crs.reuma_riesembarazoId"
                                Data="@crs.NuncaSiempre"
                                Style="width: 100%">
                </RadzenDropDown>
            </RadzenCard>
            <RadzenCard>
                <RadzenLabel Text="¿Con que frecuencia su reumatólogo le ha dicho de los riesgos de tomar algunos medicamentos durante el embarazo?" Size="H3" class="rz-card-title" />
                <RadzenDropDown TValue="int" TextProperty="Key" ValueProperty="Value"
                @bind-Value="@crs.reuma_riesgomedId"
                                Data="@crs.NuncaSiempre"
                                Style="width: 100%">
                </RadzenDropDown>
            </RadzenCard>
        </RadzenCard>


        @*Sexualidads*@
        <RadzenLabel Text="SECCIÓN 4: SEXUALIDAD" Size="H2" class="rz-card-title" />
        <RadzenCard>
            <RadzenCard>
                <RadzenLabel Text="¿Qué edad tenía usted cuando tuvo su primera relación sexual?" />
                <RadzenDropDown TValue="int" TextProperty="Key" ValueProperty="Value"
                @bind-Value="@crs.anticon_edadprimeraId"
                                Data="@crs.anticon_edadprimera"
                                Style="width: 100%">
                </RadzenDropDown>
            </RadzenCard>
            @if(crs.anticon_edadprimeraId!=4 )
            {
                <RadzenCard>
                    <RadzenLabel Text="En su primera relación sexual, ¿Qué fue lo que usted o su pareja hicieron o usaron para evitar un embarazo o una infección de transmisión sexual?" Size="H3" class="rz-card-title" />
                    <p><RadzenLabel Text="marque los que usted considere" /></p>
                    <RadzenCard>
                        <RadzenLabel Component="anticon_primerapro16" Text="No quiero responder" Style="margin-left: 8px; vertical-align: middle;" />
                        <RadzenCheckBox @bind-Value=@crs.anticon_primerapro16 Name="anticon_primerapro16" TValue="bool" />
                    </RadzenCard>
                    @if (crs.anticon_primerapro16==false)
                    {
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro1" Text="No usamos nada" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro1 Name="anticon_primerapro1" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro2" Text="Pastillas anticonceptivas" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro2 Name="anticon_primerapro2" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro3" Text="Inyecciones o ampolletas anticonceptivas" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro3 Name="anticon_primerapro3" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro4" Text="Implante anticonceptivo (subdérmico) o Norplant" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro4 Name="anticon_primerapro4" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro5" Text="Parche anticonceptivo" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro5 Name="anticon_primerapro5" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro6" Text="Anillo vaginal (NuvaRing)" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro6 Name="anticon_primerapro6" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro7" Text="DIU, dispositivo o aparato (de cobre)" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro7 Name="anticon_primerapro7" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro8" Text="DIU, dispositivo con levornogestrel (Mirena)" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro8 Name="anticon_primerapro8" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro9" Text="Condón o preservativo masculino" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro9 Name="anticon_primerapro9" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro10" Text="Condón o preservativo femenino" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro10 Name="anticon_primerapro10" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro11" Text="Óvulos, jaleas o espumas anticonceptivas" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro11 Name="anticon_primerapro11" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro12" Text="Ritmo, calendario, Billings o abstinencia periódica" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro12 Name="anticon_primerapro12" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro13" Text="Retiro o coito interrumpido" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro13 Name="anticon_primerapro13" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro14" Text="Píldora del día siguiente o anticoncepción de emergencia" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro14 Name="anticon_primerapro14" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            <RadzenLabel Component="anticon_primerapro15" Text="Otro método" Style="margin-left: 8px; vertical-align: middle;" />
                            <RadzenCheckBox @bind-Value=@crs.anticon_primerapro15 Name="anticon_primerapro15" TValue="bool" />
                        </RadzenCard>
                        <RadzenCard>
                            @if (crs.anticon_primerapro15)
                            {
                                    <RadzenTextBox @bind-Value="@crs.anticon_primeraprototro" Style="width: 100%; margin-top: 10px;" Placeholder="Otro" />
                            }
                        </RadzenCard>
                    }
                </RadzenCard>
                @if(crs.anticon_primerapro1)
                {
                     <RadzenCard>
                        <RadzenLabel Text="¿Cuál fue la razón principal por la que usted o su pareja no hicieron o usaron algo para protegerse o evitar un embarazo?" Size="H3" class="rz-card-title" />
                        <RadzenDropDown TValue="int" TextProperty="Key" ValueProperty="Value"
                        @bind-Value="@crs.anticon_primerarazonId"
                                        Data="@crs.anticon_primerarazon"
                                        Style="width: 100%">
                        </RadzenDropDown>
                        @if (crs.anticon_primerarazonId == 8)
                        {
                            <RadzenTextBox @bind-Value="@crs.anticon_primerarazonotro" Style="width: 100%; margin-top: 10px;" Placeholder="Otro" />
                        }
                    </RadzenCard>               
                }
                <RadzenLabel Text="¿Cuándo fue la última vez que usted tuvo relaciones sexuales?" Size="H3" class="rz-card-title" />
                <div class="row justify-content-center">
                    <RadzenCard>
                        <RadzenLabel Text="Prefiere decir" Size="H4" />
                        <RadzenCheckBox Text="Prefiere decir" @bind-Value="crs.ultimarelaciones_nodecir"></RadzenCheckBox>
                    </RadzenCard>
                </div>
                <div class="row justify-content-center">
                    <RadzenCard>
                        <div class="col-lg-6 col-xl-4 p-3">
                            <RadzenLabel Text="Días" Size="H4" Visible="@crs.ultimarelaciones_nodecir" />
                            <RadzenNumeric @bind-Value=@crs.ultimarelaciones_dias Name="ultimarelaciones_dias" Visible="@crs.ultimarelaciones_nodecir" />
                        </div>
                        <div class="col-lg-6 col-xl-4 p-3">
                            <RadzenLabel Text="Semanas" Size="H4" Visible="@crs.ultimarelaciones_nodecir" />
                            <RadzenNumeric @bind-Value=@crs.ultimarelaciones_semanas Name="ultimarelaciones_semanas" Visible="@crs.ultimarelaciones_nodecir" />
                        </div>
                        <div class="col-lg-6 col-xl-4 p-3">
                            <RadzenLabel Text="Meses" Size="H4" Visible="@crs.ultimarelaciones_nodecir" />
                            <RadzenNumeric @bind-Value=@crs.ultimarelaciones_meses Name="ultimarelaciones_meses" Visible="@crs.ultimarelaciones_nodecir" />
                        </div>
                        <div class="col-lg-6 col-xl-4 p-3">
                            <RadzenLabel Text="Años" Size="H4" Visible="@crs.ultimarelaciones_nodecir" />
                            <RadzenNumeric @bind-Value=@crs.ultimarelaciones_anos Name="ultimarelaciones_anos" Visible="@crs.ultimarelaciones_nodecir" />
                        </div>
                    </RadzenCard>
                </div>            
            }

            <RadzenCard>
                <RadzenLabel Text="En los últimos 12 meses, ¿usted ha escuchado o visto información de métodos anticonceptivos en..." Size="H3" class="rz-card-title" />
                <RadzenLabel Text="marque los que usted considere" />
                <RadzenCard>
                    <RadzenLabel Component="Radio" Text="Radio?" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenCheckBox @bind-Value=@crs.Radio Name="Radio" TValue="bool" />
                </RadzenCard>
                <RadzenCard>
                    <RadzenLabel Component="Television" Text="Televisión?" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenCheckBox @bind-Value=@crs.Television Name="Television" TValue="bool" />
                </RadzenCard>
                <RadzenCard>
                    <RadzenLabel Component="Periodico" Text="Periódico o revistas?" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenCheckBox @bind-Value=@crs.Periodico Name="Periodico" TValue="bool" />
                </RadzenCard>
                <RadzenCard>
                    <RadzenLabel Component="Internet" Text="Internet?" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenCheckBox @bind-Value=@crs.Internet Name="Internet" TValue="bool" />
                </RadzenCard>
                <RadzenCard>
                    <RadzenLabel Component="RedesSociales" Text="Redes sociales?" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenCheckBox @bind-Value=@crs.RedesSociales Name="RedesSociales" TValue="bool" />
                </RadzenCard>
                <RadzenCard>
                    <RadzenLabel Component="Carteles" Text="Carteles, folletos o trípticos?" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenCheckBox @bind-Value=@crs.Carteles Name="Carteles" TValue="bool" />
                </RadzenCard>
                <RadzenCard>
                    <RadzenLabel Component="Platicas" Text="Pláticas en centros de salud, hospitales o consultorios?" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenCheckBox @bind-Value=@crs.Platicas Name="Platicas" TValue="bool" />
                </RadzenCard>
                <RadzenCard>
                    <RadzenLabel Component="otro_medio" Text="en algún otro medio o lugar? ¿Cuál?" Style="margin-left: 8px; vertical-align: middle;" />
                    <RadzenCheckBox @bind-Value=@crs.otro_medio Name="otro_medio" TValue="bool" />
                </RadzenCard>
                @if (crs.otro_medio)
                {
                    <RadzenCard>
                        <RadzenTextBox @bind-Value="@crs.otro_medio_text" Style="width: 100%; margin-top: 10px;" Placeholder="Otro" />
                    </RadzenCard>
                }
            </RadzenCard>
            @if (crs.RedesSociales)
            {
                <RadzenCard>
                    <RadzenLabel Text="¿Cuáles?" Size="H3" class="rz-card-title" />
                    <RadzenLabel Text="marque los que utilice" />
                    <RadzenCard>
                        <RadzenLabel Component="Facebook" Text="Facebook" Style="margin-left: 8px; vertical-align: middle;" />
                        <RadzenCheckBox @bind-Value=@crs.Facebook Name="Facebook" TValue="bool" />
